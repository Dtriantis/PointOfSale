{
  "swagger": "2.0",
  "info": {
    "description": "This is the swagger file which defines the Point of sale API structure",
    "version": "1.0.0",
    "title": "Swagger Pos Menu Server"
  },
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "basePath": "/",
  "paths": {
    "/menuItems": {
      "get": {
        "operationId": "menuItems.read",
        "tags": [
          "Menu Items"
        ],
        "summary": "The menu data structure supported by the server application",
        "description": "Read the list of food items",
        "responses": {
          "200": {
            "description": "Successful read menu list operation",
            "schema": {
              "type": "array",
              "items": {
                "properties": {
                  "id": {
                    "type": "integer"
                  },
                  "description": {
                    "type": "string"
                  },
                  "price": {
                    "type": "integer"
                  },
                  "quantity": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}